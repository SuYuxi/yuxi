Binary Search 
def binary(target, row): #取小于等于target的最大整数，floor和mid+1很重要
	l, r = 0, len(row)-1
	while(l < r):
		mid = (l + r) >> 1 #floor
		if(target > row[mid]):
			l = mid + 1
		else:
			r = mid 
	return l

def binary(target, row): #取大于等于target的最小整数，ceil和mid-1很重要
	l, r = 0, len(row)-1
	while(l < r):
		mid = math.ceil((l + r) / 2) 
		if(target < row[mid]):
			r = mid - 1 
		else:
			l = mid
	return l
